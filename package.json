{
  "name": "vue-drag-drop-snap",
  "version": "1.0.0",
  "description": "vuejs drag-drop-snap library",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrfade/vue-drag-drop-snap.git"
  },
  "main": "dist/vue-drag-drop-snap.umd.js",
  "module": "dist/vue-drag-drop-snap.esm.js",
  "unpkg": "dist/vue-drag-drop-snap.min.js",
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "build": "npm run build:unpkg && npm run build:es && npm run build:umd",
    "build:umd": "rollup --config rollup.package.config.js --format umd --file dist/vue-drag-drop-snap.umd.js",
    "build:es": "rollup --config rollup.package.config.js --format es --file dist/vue-drag-drop-snap.esm.js",
    "build:unpkg": "rollup --config rollup.package.config.js --format iife --file dist/vue-drag-drop-snap.min.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "author": "Enes Solak",
  "license": "MIT",
  "peerDependencies": {
    "vue": "2.x"
  },
  "devDependencies": {
    "@vue/cli-service": "^5.0.4",
    "babel-preset-env": "^1.7.0",
    "minimist": "^1.2.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.2",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-fill-html": "^1.1.0",
    "rollup-plugin-hash": "^1.3.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^4.3.0",
    "vue": "^2.5.16",
    "vue-template-compiler": "^2.5.16",
    "vuepress": "^1.9.7"
  }
}
